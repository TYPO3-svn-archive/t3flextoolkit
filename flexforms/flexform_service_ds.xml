<T3DataStructure>
 <meta>
       <langDisable>1</langDisable>
 </meta>
 <sheets>
		<sDEF>
	<ROOT>
		<TCEforms>
			<sheetTitle>amfphpcode</sheetTitle>
			<description>Select the HTML element on the page which you want to be the overall container element for the template.</description>
		</TCEforms>
		<type>array</type>
		<el>	
   <amfconnections>
		<TCEforms>
			<label>LLL:EXT:t3flextoolkit/locallang_db.xml:query</label>
			<config>
				<type>group</type>
		        <internal_type>db</internal_type>
		        <allowed>tx_t3flextoolkit_query</allowed>
		        <size>5</size>
		        <maxitems>10</maxitems>
		        <minitems>0</minitems>
		        <show_thumbs>1</show_thumbs>
			</config>
		</TCEforms>
	</amfconnections>			
			<templatefiles>
				<TCEforms>
					<label>templates selection</label>
					<config>
						<type>select</type>
						<items type="array">													
						</items>
						<fileFolder>fileadmin/flexFiles/templates/</fileFolder>
						<fileFolder_extList>php</fileFolder_extList>
						<fileFolder_recursions >0</fileFolder_recursions>							
							<minitems>0</minitems>
							<maxitems>1</maxitems>			
					
								<wizards type="array">
							<uproc>
								<type>userFunc</type>
								<userFunc>user_codeAmfServiceEditor->user_button</userFunc>													
								<params>
									<wrapTag>u</wrapTag>
								</params>	
							</uproc>
			 </wizards> 
			 </config>
				</TCEforms>
			</templatefiles>	
			
			
			
			
			
				<editor>				
				<TCEforms>
				<label>actionScript</label>				
					<config>
						<type>text</type>
						<cols>100</cols>
						<rows>10</rows>						
						<eval>trim</eval>
									 
				 		<wizards type="array">
							<uproc>
								<type>userFunc</type>
								<userFunc>user_codeAmfServiceEditor->user_TCAform_procWizard</userFunc>													
								<params>
									
									<codeLang>php</codeLang>
									<langExt>php</langExt>
		</params>	
							</uproc>
			 </wizards>
							  
				</config>				
				</TCEforms>
			</editor>	
			
		</el>
	</ROOT>
	</sDEF>

  </sheets>
</T3DataStructure>

